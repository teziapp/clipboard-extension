{
  "manifest_version": 3,
  "name": "Note-it",
  "description": "Tool to help make the world of Financial Managers & Knowledge-miners easy.",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "EQUITY_L.csv",
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "identity",
    "storage",
    "tabs"
  ],
  "oauth2": {
    "client_id": "650198074640-ndskhk7q7u831es5bvjg4sc1s2e0rgn7.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.currentonly",
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "host_permissions": [
    "https://tezi-extension.hamzaravani4.workers.dev/*"
  ]
}